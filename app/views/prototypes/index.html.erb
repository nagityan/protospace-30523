<main class="main">
  <div class="inner">
    
    <%# ログインしているときは以下を表示する %>
    <%if user_signed_in? %>
      <div class="greeting"> 
        <%= "こんにちは"%>
        <%= link_to "#{current_user.name}さん", "users/#{current_user.id}", class: :greeting__link%>
        
      </div>
    <% end %>
    <div class="card__wrapper">
      <%=render partial:"prototype", collection:@prototypes%>
    </div>
  </div>
</main>
